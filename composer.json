{
    "name": "mrnoone35/neo4j-php-ogm",
    "description": "The Neo4j PHP OGM",
    "keywords": [],
    "homepage": "https://github.com/giudicelli/neo4j-php-ogm",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Giudicelli"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-ds": "*",
        "doctrine/common": "^3.1",
        "laudis/neo4j-php-client": "^1.3",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "conflict": {
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Neo4j\\OGM\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Neo4j\\OGM\\Tests\\": "tests/",
            "Neo4j\\OGM\\Examples\\": "examples/"
        }
    }
}
